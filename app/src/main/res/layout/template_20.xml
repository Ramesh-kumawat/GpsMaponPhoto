            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_19sdp"
            android:layout_marginTop="@dimen/_6sdp"
            android:gravity="center"
            android:text="37 F"
            android:visibility="gone"
            android:textColor="@color/white"
            android:textSize="@dimen/_10sdp"
            app:layout_constraintStart_toStartOf="@+id/txt_add"
            app:layout_constraintTop_toBottomOf="@+id/txt_add" />

        <TextView
            android:id="@+id/txt_humidity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_6sdp"
            android:layout_marginEnd="@dimen/_29sdp"
            android:gravity="center"
            android:text="37 F"
            android:visibility="gone"
            android:textColor="@color/white"
            android:textSize="@dimen/_10sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_add_1" />

        <TextView
            android:id="@+id/txt_full_add"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_74sdp"
            android:layout_marginTop="@dimen/_7sdp"
            android:ellipsize="end"
            android:maxLines="3"
            android:paddingLeft="@dimen/_3sdp"
            android:fontFamily="@font/segoescb"
            android:text=" 08734, USA dfn  fd,  dsg dsg gsfhdfhfg         ghfghgf "
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="@dimen/_10sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.948"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

